{
  "name": "@ondewo/vtsi-client-nodejs",
  "version": "8.0.0",
  "description": "ONDEWO Voip Telephone System Integration (VTSI) Client library for Nodejs",
  "author": "ONDEWO GmbH <office@ondewo.com>",
  "license": "Apache-2.0",
  "keywords": [
    "ondewo",
    "vtsi",
    "api",
    "client",
    "nodejs",
    "grpc"
  ],
  "homepage": "https://ondewo.com",
  "main": "public-api.js",
  "typings": "public-api.d.ts",
  "scripts": {
    "start": "npm run build",
    "build": "docker run -it -v ${PWD}:/input-volume -v ${PWD}/..:/output-volume ondewo-nodejs-proto-compiler ondewo-vtsi-api ondewo",
    "predebug": "mv ondewo-vtsi-api/googleapis/google ondewo-vtsi-api/google",
    "debug": "docker run -it --entrypoint /bin/bash -v ${PWD}:/input-volume -v ${PWD}/..:/output-volume ondewo-nodejs-proto-compiler"
  },
  "repository": {
    "type": "git",
    "directory": "https://github.com/ondewo/ondewo-vtsi-client-nodejs"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.2",
    "@types/node": "^20.12.14",
    "dotenv": "^16.4.5",
    "google-protobuf": "3.21.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "eslint": "^9.13.0",
    "global": "^4.4.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.3"
  }
}
